{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime",
    [
      "prismjs",
      {
        "languages": ["html", "css", "javascript","matlab", "php", "dart", "bash", "nginx", "sql","java","python","c","cpp","go","latex"],
        "theme": "okaidia",
        "css": true
      }
    ]
  ]
}
